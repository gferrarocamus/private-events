<h2><%= @user.name %></h2>

<h3>Events created</h3>
<h4>Past:</h4>
<ul>
<% @created_past_events.each do |event| %>
  <li><%= link_to event.name, event_path(event.id) %></li>
<% end %>
</ul>
<h4>Upcoming:</h4>
<ul>
<% @created_upcoming_events.each do |event| %>
  <li><%= link_to event.name, event_path(event.id) %></li>
<% end %>
</ul>

<h3>Events attended</h3>
<h4>Past:</h4>
<ul>
<% @attended_past_events.each do |event| %>
  <li><%= link_to event.name, event_path(event.id) %></li>
<% end %>
</ul>
<h4>Upcoming:</h4>
<ul>
<% @attended_upcoming_events.each do |event| %>
  <li><%= link_to event.name, event_path(event.id) %></li>
<% end %>
</ul>