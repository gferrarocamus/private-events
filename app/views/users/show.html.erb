<%= @user.name %><br>

<% if !@user.created_events.empty? %>
<h3>My events</h3>
<ul>
<% @user.created_events.each do |event| %>
  <li><%= link_to event.name, event_path(event.id) %></li>
<% end %>
</ul>
<% end %>

<% if !@previous_events.empty? %>
  <h3>Previous events:</h3>
  <ul>
  <% @previous_events.each do |events| %>
    <li><%= events.name %></li>
  <% end %>
  </ul>
<% end %>

<% if !@upcoming_events.empty? %>
  <h3>Upcoming events:</h3>
  <ul>
  <% @upcoming_events.each do |events| %>
    <li><%= events.name %></li>
  <% end %>
  </ul>
<% end %>

